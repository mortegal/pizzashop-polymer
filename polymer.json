    "src/pizza/ps-app-pizza.html",
    "src/pizza/add/ps-pizza-add.html",
    "src/pizza/details/ps-pizza-details.html",
    "src/core/pages/ps-not-found.html"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/webcomponents-loader.js",
